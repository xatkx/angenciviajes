.row 
            each datos in dataViajes
                .col-md-6.col-lg-4.mt-4 
                    .card
                        img.card-img-top( src=`/img/destinos_${datos.imagen}.jpg`)
                        .card-body
                            h5.card-title #{datos.titulo}
                            span
                                p
                                    span(class="badge badge-pill badge-success")  $ #{datos.precio}
                                
                                    span.text-muted   #{datos.disponibles} Disponibles
                            p
                                svg.icon.icon-tabler.icon-tabler-calendar-event(xmlns='http://www.w3.org/2000/svg' width='44' height='44' viewbox='0 0 24 24' stroke-width='1.5' stroke='#fd0061' fill='none' stroke-linecap='round' stroke-linejoin='round')
                                    path(stroke='none' d='M0 0h24v24H0z' fill='none')
                                    rect(x='4' y='5' width='16' height='16' rx='2')
                                    line(x1='16' y1='3' x2='16' y2='7')
                                    line(x1='8' y1='3' x2='8' y2='7')
                                    line(x1='4' y1='11' x2='20' y2='11')
                                    rect(x='8' y='15' width='2' height='2')

                                span #{datos.fecha_ida} | #{datos.fecha_vuelta}
                            p.card-text #{datos.descripcion.substr(0,103)+'....'}
                            a.btn.btn-success.btn-block(href=`/viajes/${datos.slug}`) Mas informacion